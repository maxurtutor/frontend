{
  "name": "frontend",
  "version": "1.0.0",
  "description": "Frontend Tutorial",
  "author": "Maxim Yunusov",
  "license": "MIT",
  "main": "build/dist/frontend.browser.min.js",
  "scripts": {
    "start": "webpack-dev-server --config config/webpack.config.js",
    "lint": "eslint src",
    "test": "jest",
    "prebuild": "rimraf build",
    "build-dev": "webpack -d --config config/webpack.config.js",
    "build-prod": "cross-env NODE_ENV=production webpack -p --config config/webpack.config.js",
    "postbuild": "copyfiles index.html dist"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/src/styles/"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/src/styles/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maxurtutor/frontend.git"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^21.2.0",
    "babel-loader": "7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.24.1",
    "copyfiles": "^1.2.0",
    "cross-env": "5.0.0",
    "css-loader": "^0.28.7",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-react": "^6.10.3",
    "exports-loader": "^0.6.4",
    "file-loader": "^1.1.4",
    "graceful-fs": "^4.1.11",
    "http-server": "^0.10.0",
    "identity-obj-proxy": "^3.0.0",
    "imports-loader": "^0.7.1",
    "jest": "^21.1.0",
    "jest-cli": "^21.2.1",
    "minimatch": "^3.0.4",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.6",
    "precss": "^2.0.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-test-renderer": "^16.0.0",
    "regenerator-runtime": "^0.11.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.19.0",
    "stylelint": "^8.1.1",
    "stylelint-webpack-plugin": "^0.9.0",
    "uglify": "^0.1.5",
    "url-loader": "^0.6.1",
    "webpack": "3.5.2",
    "webpack-dev-server": "^2.7.1",
    "webpack-notifier": "1.5.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "es6-promise": "^4.1.1",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "whatwg-fetch": "^2.0.3"
  }
}
