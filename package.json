    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "copyfiles": "^1.2.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-react": "^6.10.3",
    "file-loader": "^1.1.5",
    "flow-bin": "^0.57.3",
    "html-webpack-plugin": "^2.30.1",
