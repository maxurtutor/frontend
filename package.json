{
  "name": "frontend",
  "version": "1.0.0",
  "description": "Frontend Template",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.config.js --progress",
    "lint-js": "eslint src",
    "lint-css": "stylelint \"**/*.css \" --verbose stylelint --config ./.stylelintrc",
    "test": "jest",
    "prebuild": "rimraf build",
    "build-dev": "cross-env NODE_ENV=development webpack -d --config config/webpack.config.js --progress",
    "build-prod": "cross-env NODE_ENV=production webpack -p --config config/webpack.config.js --progress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxurtutor/frontend.git"
  },
  "keywords": [
    "webpack",
    "frontend",
    "react",
    "redux",
    "npm",
    "babel"
  ],
  "author": "Maxim Yunusov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maxurtutor/frontend/issues"
  },
  "homepage": "https://github.com/maxurtutor/frontend#readme",
  "dependencies": {
    "@types/react": "^16.0.21",
    "@types/react-dom": "^16.0.2",
    "@types/react-redux": "^5.0.12",
    "@types/redux-logger": "^3.0.5",
    "@types/webpack-env": "^1.13.2",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "es6-promise": "^4.1.1",
    "jss": "^9.0.0",
    "material-ui": "^1.0.0-beta.18",
    "material-ui-icons": "^1.0.0-beta.17",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-tap-event-plugin": "^3.0.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "typeface-roboto": "0.0.41"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.5",
    "awesome-typescript-loader": "^3.3.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-es6-promise": "^1.1.1",
    "babel-plugin-syntax-flow": "^6.18.0",
